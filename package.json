{
  "name": "hybrid-router",
  "version": "1.0.0",
  "description": "Hybrid Router System for Ubuntu Linux with Raspberry Pi 3B+ support",
  "main": "api/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "dev:api": "nodemon api/server.js",
    "dev:web": "cd web && npm run dev",
    "build": "cd web && npm run build",
    "start": "node api/server.js",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "router",
    "networking",
    "raspberry-pi",
    "ubuntu",
    "linux",
    "firewall",
    "vpn",
    "access-point"
  ],
  "author": "Hybrid Router Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "sqlite3": "^5.1.6",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^6.8.1",
    "ws": "^8.13.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "winston": "^3.10.0",
    "joi": "^17.9.2",
    "python-shell": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "jest": "^29.6.1",
    "eslint": "^8.44.0",
    "typescript": "^5.1.6",
    "@types/node": "^20.4.2",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/ws": "^8.5.5",
    "@types/multer": "^1.4.7",
    "@types/node-cron": "^3.0.8"
  }
}